{
  "compilerOptions": {
    "outDir": "./dist/",
    "sourceMap": true,
    "noImplicitAny": false,
    "module": "commonjs",
    "lib": ["ES6", "ES2016", "dom"],
    "target": "es6",
    "declaration": true,
    "rootDir": "./",
    "typeRoots": [
      "node_modules/@types"
    ]
  },
  "exclude": [
    "node_modules",
    "build",
    "webpack"
  ],
  "include": [
    "./**/*"
  ]
}
